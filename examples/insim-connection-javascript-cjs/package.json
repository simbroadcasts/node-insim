{
  "name": "node-insim-example-insim-connection-javascript-cjs",
  "version": "0.0.0",
  "description": "Node InSim - InSim connection (JavaScript + CommonJS)",
  "main": "dist/index.js",
  "type": "commonjs",
  "repository": "git@github.com:mkapal/lfs-race-insim.git",
  "author": "Martin Kapal <flamecze@gmail.com>",
  "private": true,
  "scripts": {
